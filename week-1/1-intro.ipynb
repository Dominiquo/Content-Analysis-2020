    "def crawl_pages(base_link, current_links=[], link_max=65):\n",
    "    valid_dirs = {'article', 'sports', 'business', 'lifestyle', 'pueblowestview',\n",
    "                  'entertainment', 'opinion', 'news', 'letters', 'thingstodo'}\n",
    "    if len(current_links) > link_max:\n",
    "        return current_links\n",
    "    page_text = retrieve_page_text(base_link)\n",
    "    chieftain_re = 'https://www.chieftain.com/[\\w\\d\\S]+?(?=\")'\n",
    "    p = re.compile(chieftain_re)\n",
    "    all_finds = p.findall(page_text)\n",
    "    for found_link in all_finds:\n",
    "        if found_link.split('/')[3] in valid_dirs:\n",
    "            if found_link not in current_links: #yes, I know it can get slow\n",
    "                current_links.append(found_link)\n",
    "    for link in current_links:\n",
    "        crawl_pages(link, current_links, link_max)\n",
    "    return all_finds"
   ]
