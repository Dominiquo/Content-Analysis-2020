    "def retrieve_article_links(page_text):\n",
    "    #from inspection, I see that all of the texts are either in h1 or h3 tags with the \n",
    "    #class='headline'\n",
    "    h3_tag = 'h3'\n",
    "    h1_tag = 'h1'\n",
    "    hline = 'headline'\n",
    "    href = 'href'\n",
    "    all_links = []\n",
    "    try:\n",
    "        soup = bs4.BeautifulSoup(page_text, 'html.parser')\n",
    "        h1_list = soup.find_all(h1_tag, {'class': hline})\n",
    "        h3_list = soup.find_all(h3_tag, {'class': hline})\n",
    "        all_headlines = h1_list + h3_list\n",
    "        for hline in all_headlines:\n",
    "            h_tag_bs = hline.find('a')\n",
    "            if h_tag_bs:\n",
    "                all_links.append(h_tag_bs.get(href))\n",
    "        return all_links\n",
    "    except Exception as e:\n",
    "        print(e)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def extract_author(page_text):\n",
    "    author_re = '\"author\": \\[[^\\]]+'\n",
    "    rule_re = '{[\\w\\S\\s]+}'\n",
    "    name_key = 'name'\n",
    "    p = re.compile(author_re)\n",
    "    rule2 = re.compile(rule_re)\n",
    "    author_sect = p.search(page_text).group(0)\n",
    "    dict_val = rule2.search(author_sect).group(0)\n",
    "    return json.loads(dict_val)[name_key]\n",
    "\n",
    "def extract_time(page_text):\n",
    "    date_re = '\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}'\n",
    "    p = re.compile(date_re)\n",
    "    return p.search(page_text).group(0)\n",
    "\n",
    "def extract_body(page_text):\n",
    "    soup = bs4.BeautifulSoup(page_text, 'html.parser')\n",
    "    body_tag = 'article-body'\n",
    "    for div_sect in soup.find_all('div', {'class':'inner'}):\n",
    "        if div_sect.find('div', {'class': body_tag}):\n",
    "            for p_sect in div_sect.find_all('p'):\n",
    "                if len(p_sect.text) > 200*5:\n",
    "                    return p_sect.text\n",
    "    return False\n",
    "\n",
    "def extract_twitter(page_text):\n",
    "    twitter_re = 'twitter.com/\\w+'\n",
    "    p = re.compile(twitter_re)\n",
    "    return p.findall(page_text)\n",
    "\n",
    "def extract_emails(page_text):\n",
    "    chieftain_email_re = '\\w+@chieftain.com'\n",
    "    p = re.compile(chieftain_email_re)\n",
    "    return p.findall(page_text)"
   ]
  },
