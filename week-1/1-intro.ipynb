    "def build_df(page_links):\n",
    "    email = 'email'\n",
    "    twitter = 'twitter'\n",
    "    article_text = 'article_text'\n",
    "    author = 'author'\n",
    "    date = 'date'\n",
    "    df_dict = {email: [], twitter: [], article_text: [], author: [], date: []}\n",
    "    \n",
    "    for link in page_links:\n",
    "        article_source = retrieve_page_text(link)\n",
    "        df_dict[email].append(extract_email(article_source))\n",
    "        df_dict[twitter].append(extract_twitter(article_source))\n",
    "        article_body = extract_body(article_source)\n",
    "        df_dict[article_text].append(article_body)\n",
    "        df_dict[author].append(extract_author(article_source))\n",
    "        df_dict[date].append(extract_time(article_source))\n",
    "\n",
    "    return pandas.DataFrame(df_dict)"
   ]
